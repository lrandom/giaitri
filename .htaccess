Options +FollowSymLinks  All -Indexes
<IfModule mod_rewrite.c>    
   RewriteEngine On
   RewriteBase /thugian/
   RewriteRule ^index\.php$ - [L]
   RewriteCond %{REQUEST_FILENAME} !-f
   RewriteCond %{REQUEST_FILENAME} !-d
   RewriteRule . index.php [L]	
</IfModule>
<IfModule !mod_rewrite.c>
    ErrorDocument 404 thugian/index.php
</IfModule> 

